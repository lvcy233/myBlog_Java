<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dev.blog.mapper.TagsMapper">

    <select id="queryByTag" resultType="java.lang.Integer">
        select count(1) from tags where tag = #{tag};
    </select>
</mapper>
